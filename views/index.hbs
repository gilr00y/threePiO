<div class="container-fluid">
  <div class="row">
    <div class="col-xs-12 col-md-6">
      <h2>Command History</h2>
      <div class="command-history">
      </div>
    </div>

    <div class="col-xs-12 col-md-6">
      <h2>Command Queue</h2>

      <div class="field-group">
        <label>Delay between commands:</label>
        <select data-js="select-timeout">
          <option value="0">0</option>
          <option value="0.5">0.5</option>
          <option value="1">1</option>
          <option value="2">2</option>
          <option value="3">3</option>
          <option value="4">4</option>
          <option value="5">5</option>
          <option value="10">10</option>
        </select>
      </div>

      <div class="command-queue">
      </div>
    </div>
  </div>
</div>

<div class="container-fluid">
  <div class="user-input">
    <input type="text" class="name-input" placeholder="User Name"/>
    <input type="text" class="command-input" placeholder="Enter Command"/>
    <button class="btn btn-default command-btn">Submit</button>
  </div>
</div>
